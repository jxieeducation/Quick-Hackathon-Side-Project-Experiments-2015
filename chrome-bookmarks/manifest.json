{
  "name": "Bookmarks Plz",
  "description": "Displays a list of all the links in your bookmark for copy and paste",
  "version": "0.1",
  "manifest_version": 2,
  "icons": { 
    "16": "bookmark-16.png", 
    "128": "bookmark-128.png" 
  },
  "browser_action": {
    "default_icon": "bookmark-16.png"
  },
  "background": {
    "scripts": ["run.js"],
    "persistent": false
  },
  "permissions": [
    "bookmarks",
    "tabs"
  ],
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}